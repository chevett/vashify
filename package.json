{
  "name": "vashify",
  "version": "0.3.2",
  "description": "compile vash templates for use with browserify",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/browserify/bin/cmd.js -t ./index.js ./example/entry.js",
    "test": "$(test \"$(./node_modules/browserify/bin/cmd.js -t ./index.js ./example/entry.js|node)\" = \"Hi Skeet Ulrich.  wow.\") && echo 'PASS' || ! echo 'FAIL'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:chevett/vashify.git"
  },
  "keywords": [
    "vash",
    "razor",
    "template",
    "browserify"
  ],
  "author": "mike chevett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chevett/vashify/issues"
  },
  "homepage": "https://github.com/chevett/vashify",
  "dependencies": {
    "browserify-transform-tools": "~1.2.1",
    "mkdirp": "~0.3.5",
    "re-require": "0.0.0",
    "resolve": "~0.6.1",
    "vash": "~0.7.6-3"
  },
  "devDependencies": {
    "browserify": "~3.18.0"
  }
}
